{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Cargo launch",
			"cargo": {
				"args": [
					"build",
					"--lib"
				]
			},
			"args": []
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'antbyte'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=antbyte"
				],
				"filter": {
					"name": "antbyte",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'antbyte'",
			"cargo": {
				"args": [
					"build",
					"--bin=antbyte",
					"--package=antbyte"
				],
				"filter": {
					"name": "antbyte",
					"kind": "bin"
				}
			},
			"args": ["test.ant", "--auto-step"],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'antbyte'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=antbyte",
					"--package=antbyte"
				],
				"filter": {
					"name": "antbyte",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}